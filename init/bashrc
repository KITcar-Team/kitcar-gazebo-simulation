echo 'exporting kitcar-gazebo-simulation path variables'

export KITCAR_GAZEBO_SIM_PATH=$KITCAR_REPO_PATH/kitcar-gazebo-simulation

export GAZEBO_RESOURCE_PATH=$KITCAR_GAZEBO_SIM_PATH/models:$GAZEBO_RESOURCE_PATH
export GAZEBO_MODEL_PATH=$KITCAR_GAZEBO_SIM_PATH/models/env_db:$KITCAR_GAZEBO_SIM_PATH/models/model_db:$GAZEBO_MODEL_PATH

export ROS_PACKAGE_PATH=$ROS_PACKAGE_PATH:$KITCAR_GAZEBO_SIM_PATH/src

export KITCAR_SIM_DATA_PATH=$KITCAR_REPO_PATH/kitcar-simulation-data

source $KITCAR_GAZEBO_SIM_PATH/devel/setup.bash
